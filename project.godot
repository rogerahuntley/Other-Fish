config_version=4

_global_script_classes=[ {
"base": "Sprite3D",
"class": "CharacterSprite",
"language": "GDScript",
"path": "res://Scripts/Character.gd"
} ]
_global_script_class_icons={
"CharacterSprite": ""
}

[application]

config/name="OtherFish"
run/main_scene="res://Scenes/MainMenu.tscn"
